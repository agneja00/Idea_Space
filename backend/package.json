    "@types/express": "^4.17.23",
    "@types/node": "^24.0.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1"
  }
}
